{
	"name": "@public-ui/library-app",
	"version": "0.0.0",
	"scripts": {
		"build": "rimraf dist && cross-env NODE_ENV=production webpack",
		"build:deps": "pnpm --filter @public-ui/library-app^... build",
		"format": "prettier --check src",
		"lint": "eslint \"{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}\"",
		"serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
		"start": "npm run serve -- --open",
		"unused": "ts-prune -e src",
		"prepare": "npm-run-all2 prepare:*",
		"prepare:components-assets": "cpy \"node_modules/@public-ui/components/assets/**/*\" public/assets --dot",
		"prepare:theme-assets": "cpy \"node_modules/@public-ui/theme-default/assets/**/*\" public/assets --dot"
	},
	"dependencies": {
		"@leanup/stack": "1.3.54",
		"@leanup/stack-react": "1.3.54",
		"@leanup/stack-webpack": "1.3.54",
		"@public-ui/components": "3.0.5",
		"@public-ui/library-components": "workspace:*",
		"@public-ui/library-react": "workspace:*",
		"@public-ui/react": "3.0.5",
		"adopted-style-sheets": "1.1.8",
		"@types/node": "22.15.32",
		"@types/react": "18.3.23",
		"@types/react-dom": "18.3.7",
		"ajv": "8.17.1",
		"cpy-cli": "6.0.0",
		"cross-env": "10.1.0",
		"css-loader": "7.1.2",
		"esbuild-loader": "4.3.0",
		"eslint": "8.57.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-react": "7.37.5",
		"file-loader": "6.2.0",
		"formik": "2.4.6",
		"npm-run-all2": "8.0.4",
		"postcss-loader": "8.1.1",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"react-router": "7.6.2",
		"react-router-dom": "7.6.2",
		"rimraf": "6.0.1",
		"sass-loader": "16.0.5",
		"string-replace-loader": "3.2.0",
		"ts-prune": "0.10.3",
		"typescript": "5.8.3",
		"webpack": "5.99.9",
		"world_countries_lists": "2.9.0",
		"yup": "1.6.1"
	},
	"files": [
		".eslintignore",
		".eslintrc.js",
		".gitignore",
		".prettierignore",
		"dist/**",
		"prettier.config.js",
		"public/**",
		"src/**",
		"test-build.sh",
		"tsconfig.json",
		"unocss.config.ts",
		"webpack.config.js"
	]
}
